{
  "name": "en-us to ja-jp",
  "version": "0.0.5",
  "manifest_version":2,
  "description": "/en-us/ -> /ja-jp/",
  "permissions": [
    "activeTab",
    "tabs",
    "contextMenus"
  ],
  "background":{
    "scripts":  ["background.js"]
  },
  "page_action": {
    "default_icon": "off19.png",
    "default_title": "Language Change"
  },
  "icons" : {
    "128": "128.png"
  }
}